<h2 class="sr-only"><%= t('blacklight.search.search_results') %></h2>

<%- if @document_list_a.empty? %>
  <%= render partial: 'catalog/zero_results' %>
<%- else %>
  <div class="row">
    <div class="col-md-12">
      <%= render partial: 'shared/annotation', collection: @document_list_a, as: :document %>
    </div>
  </div>
  <%= render partial: 'shared/results_footer' %>
<%- end %>
